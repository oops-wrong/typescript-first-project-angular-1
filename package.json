{
  "name": "type-ng-shop",
  "private": true,
  "version": "0.1.0",
  "description": "Type shop on AngularJS",
  "repository": "",
  "license": "MIT",
  "scripts": {
    "app": "webpack --config config/app.webpack.config.js --progress",
    "dll": "npm run vendor && npm run gulp",
    "gulp": "gulp --gulpfile config/gulpfile.js copy",
    "gulp:watch": "gulp --gulpfile config/gulpfile.js watch",
    "start": "node node_modules/.bin/webpack-dev-server --content-base dist",
    "production": "webpack --config config/vendor.webpack.config.js --progress --prod && npm run gulp && webpack --config config/app.webpack.config.js --progress --prod",
    "vendor": "webpack --config config/vendor.webpack.config.js --progress",
    "watch": "npm run vendor && webpack --config config/app.webpack.config.js --watch --progress"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "awesome-typescript-loader": "^2.2.4",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "clean-webpack-plugin": "^0.1.13",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^2.22.0",
    "node-sass": "^3.10.1",
    "path": "^0.12.7",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "@types/angular": "^1.5.16",
    "@types/angular-animate": "^1.5.5",
    "@types/angular-resource": "^1.5.6",
    "@types/angular-ui-router": "^1.1.34",
    "@types/ng-dialog": "0.0.31",
    "angular": "^1.5.8",
    "angular-animate": "^1.5.8",
    "angular-resource": "^1.5.8",
    "angular-ui-router": "^0.3.1",
    "bootstrap": "^3.3.7",
    "jquery": "^3.1.0",
    "jquery-zoom": "^1.7.18",
    "ng-dialog": "^0.6.4"
  }
}
